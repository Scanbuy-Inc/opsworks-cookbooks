alias chkup='while true; do clear; uptime; sleep 1; done;'
alias chklog='tailf <%=@INSTALL_DIR%>/logs/catalina.out'
alias chkip='curl http://169.254.169.254/latest/meta-data/public-ipv4'
alias flushram='free -m && sync && echo 3 > /proc/sys/vm/drop_caches && free -m'
alias stopstart='service tomcat7 stop; flushram ; service tomcat7 start'
