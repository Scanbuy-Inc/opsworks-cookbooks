/var/log/coupons/<%=@APPNAME%>.log {
        notifempty
        copytruncate
        start 1
        postrotate
                lpath="/var/log/coupons"
                rpath=$(date +"%Y/%m/%d/")
                ext=$(date +"%Y%m%d_%H%M%S")
                [ -d $lpath/<%=@APPNAME%> ] || mkdir -p $lpath/<%=@APPNAME%>
                orig=$(find $lpath -maxdepth 1 -regextype posix-awk -regex '.*/<%=@APPNAME%>.log.[0-9]{1,2}')
                mv /$orig $lpath/<%=@APPNAME%>/<%=@APPNAME%>_$ext.log
                aws s3 cp $lpath/<%=@APPNAME%>/<%=@APPNAME%>_$ext.log s3://imgs.dcoupon.scanlife.com/logs/$rpath/<%=@APPNAME%><%=@NUMBER%>_$ext.log
        endscript
}
